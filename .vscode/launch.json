  "version": "0.2.0",
  "configurations": [
    {
      "name": "nodemon-curFile",
      "type":"node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "program": "${file}",
    },
    {
      "name": "node-curFile",
      "type":"node",
      "request": "launch",
      "program": "${file}",
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "file": "${file}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceRoot}"
    }
  ]
}